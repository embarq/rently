{
  "name": "rently",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run serve:ssr",
    "serve": "ng s -o",
    "build:app": "ng build",
    "build:app:prod": "ng build --prod",
    "build:server": "ng run rently:server --bundleDependencies none",
    "build:server:prod": "ng run rently:server:production --bundleDependencies none",
    "prebuild:server": "npm run rename-index",
    "prebuild:server:prod": "npm run rename-index",
    "serve:ssr": "STANDALONE=true node index",
    "bundle:ssr": "SERVER=true webpack --config webpack.server.config.js --progress --colors",
    "build:ssr": "npm run build:client-server && npm run bundle:ssr",
    "build:ssr:dev": "npm run build:client-server:dev && npm run bundle:ssr",
    "build:client-server:dev": "SERVER=true npm run build:app && SERVER=true npm run build:server",
    "build:client-server": "SERVER=true npm run build:app:prod && SERVER=true npm run build:server:prod",
    "build:functions": "cd functions; npm run build; cd -",
    "build:all": "npm run build:ssr && npm run build:functions",
    "build:all:dev": "npm run build:ssr:dev && npm run build:functions",
    "appconfig": "firebase functions:config:get > .runtimeconfig.json",
    "postappconfig": "cp .runtimeconfig.json functions/.runtimeconfig.json",
    "rename-index": "mv dist/browser/index.html dist/browser/index-template.html",
    "lint": "ng lint",
    "deploy": "npx firebase deploy"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.4",
    "@angular/common": "~8.2.4",
    "@angular/compiler": "~8.2.4",
    "@angular/core": "~8.2.4",
    "@angular/fire": "^5.2.1",
    "@angular/forms": "~8.2.4",
    "@angular/platform-browser": "~8.2.4",
    "@angular/platform-browser-dynamic": "~8.2.4",
    "@angular/platform-server": "~8.2.4",
    "@angular/router": "~8.2.4",
    "@fortawesome/fontawesome-free": "^5.10.2",
    "bootstrap": "^4.3.1",
    "bootswatch": "^4.3.1",
    "express": "^4.15.2",
    "firebase": "^6.6.1",
    "firebase-tools": "7.6.1",
    "ng-lazyload-image": "7.0.1",
    "ng5-slider": "1.2.4",
    "preboot": "7.0.0",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "typescript": "3.5.3",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^8.2.0",
    "@angular-devkit/architect": "<0.900 || ^0.900.0-0 || ^9.0.0-0",
    "@angular-devkit/build-angular": "~0.803.3",
    "@angular/cli": "~8.3.3",
    "@angular/compiler-cli": "~8.2.4",
    "@angular/language-service": "~8.2.4",
    "@nguniversal/express-engine": "^8.1.1",
    "@nguniversal/module-map-ngfactory-loader": "8.1.1",
    "@types/node": "^12.7.8",
    "bufferutil": "^4.0.1",
    "fuzzy": "^0.1.3",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "ts-loader": "^5.2.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "utf-8-validate": "^5.0.2",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.1.0",
    "ws": "^7.1.2",
    "xhr2": "^0.2.0"
  }
}
